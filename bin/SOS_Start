xrandr --output DVI-D-0 --off --output HDMI-0 --off --output DP-0 --primary --mode 3840x2160 --pos 0x0 --rotate normal --output DP-1 --off
wal -R &
setxkbmap -layout us -variant intl &
NetworkManager &
caffeine-indicator &
xsettingsd &
conky &
transmission-daemon &
kdeconnect-indicator &
blueman-applet &
barrier &



